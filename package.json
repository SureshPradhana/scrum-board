{
  "name": "scrum-board",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && prisma generate && prisma migrate deploy",
    "start": "next start",
    "typecheck": "tsc",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,css}\"",
    "lint": "next lint --no-cache",
    "prisma-generate": "npx prisma generate",
