    "ci:lint": "eslint --no-cache \"src/**/*.{ts,tsx,js}\"",
    "ci:prisma-migrate": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^5.4.1",
    "@vercel/postgres": "^0.5.0",
